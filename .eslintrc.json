{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double"],
        "no-console": "off",
        "no-tabs": ["error", { "allowIndentationTabs": true }],
        "no-restricted-globals": "off"
    }
}
