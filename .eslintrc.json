{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "indent": ["error", 2],
        "import/extensions": "off",
        "no-restricted-globals": "off",
        "no-undef": "off"
    }
}
