{
    "extends": ["airbnb-base"],
    "env": {
      "node": true,
      "es6": true,
      "browser": true,
      "jest": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": "error",
      "eqeqeq": "off",
      "no-restricted-globals": "off",
      "import/extensions": "off"
    }
  }